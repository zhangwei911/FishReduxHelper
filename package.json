{
	"name": "fishreduxhelper",
	"displayName": "FishReduxHelper",
	"description": "Fish Redux Helper",
	"version": "0.0.7",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"publisher": "wellchang",
	"activationEvents": [
		"onCommand:fishreduxhelper.addAction",
		"onCommand:fishreduxhelper.addParams",
		"onLanguage:dart"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fishreduxhelper.addAction",
				"title": "Fish Redux Helper: Add Action"
			},
			{
				"command": "fishreduxhelper.addParams",
				"title": "Fish Redux Helper: Add Params"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.6.4",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vsce": "^1.79.5",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {}
}
